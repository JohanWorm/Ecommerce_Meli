    "react-router-dom": "^5.3.0",
    "sass": "^1.49.9",
    "sass-loader": "^11.0.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTest.js"
